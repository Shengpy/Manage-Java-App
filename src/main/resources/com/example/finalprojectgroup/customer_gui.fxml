<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="299.0" prefWidth="255.0" spacing="10.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label style="-fx-font-size: 24.0; -fx-font-family: sans-serif; -fx-text-fill: #000000;" text="Customer Login">
         <font>
            <Font name="SansSerif" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" promptText="Username" style="-fx-font-size: 16.0; -fx-font-family: sans-serif; -fx-text-fill: #000000; -fx-background-color: #ffffff;">
         <font>
            <Font name="SansSerif" size="16.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordField" promptText="Password" style="-fx-font-size: 16.0; -fx-font-family: sans-serif; -fx-text-fill: #000000; -fx-background-color: #ffffff;">
         <font>
            <Font name="SansSerif" size="16.0" />
         </font>
      </PasswordField>
      <Button style="-fx-font-size: 16.0; -fx-font-family: sans-serif; -fx-text-fill: #000000; -fx-background-color: #ffffff; -fx-background-radius: 10.0;" text="Login">
         <font>
            <Font name="SansSerif" size="16.0" />
         </font>
      </Button>
      <Button style="-fx-font-size: 16.0; -fx-font-family: sans-serif; -fx-text-fill: #000000; -fx-background-color: #ffffff; -fx-background-radius: 10.0;" text="Add Customer">
         <font>
            <Font name="SansSerif" size="16.0" />
         </font>
      </Button>
      <Button style="-fx-font-size: 16.0; -fx-font-family: sans-serif; -fx-text-fill: #000000; -fx-background-color: #ffffff; -fx-background-radius: 10.0;" text="Remove Customer">
         <font>
            <Font name="SansSerif" size="16.0" />
         </font>
      </Button>
   </children>
</VBox>
